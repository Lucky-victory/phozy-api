{
  "name": "phozy-api",
  "version": "1.0.0",
  "description": "A simple image API for organising images in albums",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucky-victory/phozy-api.git"
  },
  "keywords": [
    "image",
    "API"
  ],
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "lucky victory <luckyvictory54@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucky-victory/phozy-api/issues"
  },
  "homepage": "https://github.com/lucky-victory/phozy-api#readme",
  "dependencies": {
    "@tensorflow/tfjs-node": "^3.18.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "knex": "^2.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.6",
    "eslint": "^8.20.0",
    "nodemon": "^1.4.1"
  }
}
